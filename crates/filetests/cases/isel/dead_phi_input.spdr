    # nextln: 0002:      %3:gpr(reg)[late] = MovRZ
    # nextln: 0003:      AluRRm(S32, Test) %4(reg)[early], %4(any)[early]
    # nextln: 0004:      %2:gpr(reg)[late] = Setcc(E) %3(tied:0)[early]
    # nextln: 0005:      AluRRm(S32, Test) %2(reg)[early], %2(any)[early]
    # nextln: 0006:      Jumpcc(Ne, block2, block3)
    # nextln:       => block2, block3
    # nextln:       block2:
    # nextln: 0007:      Ret %1($$rax)[early]
    # nextln:       block3:
    # nextln: 0008:      Ret %1($$rax)[early]

    %entry:ctrl, %p:i32 = entry
    %0:i32 = iconst 0
    %5:i32 = iconst 5
    %cmp:i32 = icmp eq %p, %0
    %dead:ctrl, %deadsel:phisel = region
